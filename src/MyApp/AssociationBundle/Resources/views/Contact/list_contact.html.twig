{# empty Twig template #}


<table>
		{% for contact in contacts %}
    <tr>
        {% include 'MyAppAssociationBundle:Contact:sum_contact.html.twig' with {'contact': contact} %}
        <td><a href="{{ path('myapp_contact_modify', { 'id': contact.id }) }}">Modifier</a></td>
        <td><a href="{{ path('myapp_contact_delete', { 'id': contact.id }) }}">Supprimer</a></td>
    </tr>
		{% else %}
    <tr><td>Aucun contacts </td></tr>
        {% endfor %}
</table>